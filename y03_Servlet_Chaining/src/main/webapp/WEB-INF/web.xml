<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>y03_Servlet_Chaining</display-name>
  <welcome-file-list>
    <welcome-file>add.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>DataCollectorServlet</servlet-name>
    <servlet-class>com.servlets.DataCollectorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DataCollectorServlet</servlet-name>
    <url-pattern>/collect</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdditionServlet</servlet-name>
    <servlet-class>com.servlets.AdditionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdditionServlet</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
</web-app>